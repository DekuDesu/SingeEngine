vertexShader: assets/shaders/debug_normal.vertexshader
fragmentShader: assets/shaders/debug_normal.fragmentshader
geometryShader: assets/shaders/debug_normal.geometryshader

# whether or not backface culling should be enabled for this shader
enableBackfaceCulling: true

# whether or not this shader should use camera perspective, (GUI elements for example shouldnt)
useCameraPerspective: true

depthTest: less