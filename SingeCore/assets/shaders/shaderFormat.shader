# the vertex shader path
vertexShader: assets/shaders/toon.vertexshader

# the fragment shader path
fragmentShader: assets/shaders/toon.fragmentshader

# whether or not backface culling is enabled for this shader
backfaceCulling: true

# whether or not blending(transparency) should be enabled for this shader
blending: true

# whether or not this shader should be rendered with the perspective of the camera(this would be false for GUI elements for example)
useCameraPerspective: false